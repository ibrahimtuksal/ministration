{% extends 'base.html.twig' %}

{% block title %}{{ corporate.title | capitalize }}{% endblock %}

{% block body %}
    <div class="container mb-5">
        <section id="features" class="text-center my-5">
            <h6 class="title display-4">{{ corporate.title }}</h6>
            <p class="grey-text w-responsive mx-auto mb-5 wow fadeIn" data-wow-delay="0.2s">
                {{ corporate.content |raw }}
            </p>

            <a class="text-center btn btn-primary " {% if global.general.isGoogletag %} onclick="return gtag_report_conversion('tel:{{ global.phone.phone }}');" {% endif%} href="tel:{{ global.phone.phone }}">{{ global.phone.title }} <i class="fas fa-phone"></i></a>
            <a class="text-center btn btn-success" {% if global.general.isGoogletag %} onclick="return gtag_report_conversion('https://wa.me/{{ global.phone.whatsapp }}');" {% endif%} target="_blank" href="https://wa.me/{{ global.phone.whatsapp }}">{{ global.phone.wpTitle }} <i class="fab fa-whatsapp"></i></a>

        </section>

    </div>


{% endblock %}
